{
    "name": "payment-service",
    "version": "1.0.0",
    "main": "index.js",
    "scripts": {
        "start": "node dist/app.js",
        "dev": "cross-env NODE_ENV=development ts-node src/app.ts",
        "build": "tsc",
        "test": "echo \"Error: no test specified\" && exit 1",
        "typeorm": "cross-env NODE_ENV=development ts-node ./node_modules/typeorm/cli.js",
        "migration:generate": "npm run typeorm -- migration:generate -d src/common/config/database.ts src/migrations/payments",
        "migration:run": "npm run typeorm -- migration:run -d src/common/config/database.ts",
        "migration:revert": "npm run typeorm migration:revert -- -d src/common/config/database.ts",
        "migration:create": "typeorm-ts-node-commonjs migration:create src/migrations/Init",
        "migration:generate:initial": "typeorm migration:generate -d src/common/config/database.ts src/migrations/Initial --pretty"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "description": "",
    "dependencies": {
        "class-validator": "^0.14.3",
        "cors": "^2.8.6",
        "dotenv": "^17.2.3",
        "express": "^5.2.1",
        "express-rate-limit": "^8.2.1",
        "express-validator": "^7.3.1",
        "helmet": "^8.1.0",
        "pg": "^8.18.0",
        "reflect-metadata": "^0.2.2",
        "typeorm": "^0.3.28"
    },
    "devDependencies": {
        "@types/cors": "^2.8.19",
        "@types/dotenv": "^6.1.1",
        "@types/express": "^5.0.6",
        "@types/node": "^25.2.0",
        "cross-env": "^10.1.0",
        "ts-node": "^10.9.2",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.9.3"
    }
}
